{
  "name": "Topology",
  "title": "Argus Topology",
  "description": "Network topology visualizer and tools.",
  "version": "1.0.0",
  "scripts": {
    "compile-sass": "sass src/sass/style.scss:dist/css/style.css",
	"compile-sass-watch": "sass src/sass/style.scss:dist/css/style.css --watch",
	"compile-temp": "handlebars src/template/ -f dist/js/app/templates.js -e hbs -m",
	"compile-temp-watch": "gulp compile-temp-watch",
    "compile-js": "babel src/js/app -d dist/js/app -s",
    "compile-js-watch": "babel src/js/app -d dist/js/app -s -w",
	"compile-all": "npm run compile-sass && npm run compile-temp && npm run compile-js",
	"livereload": "gulp livereload",
    "test": "node build/test",
    "libs-js": "node build/jsLibs",
    "libs-css": "node build/cssLibs",
    "setenv-release": "",
	"setenv-debug": "",
    "release": "npm run compile-all && node build/release",
    "fakeData": "node scripts/data",
    "test": "echo \"Error: no test specified\" && exit 1",
    "handlebars": "handlebars --help"
  },
  "author": "Mohammad Ahmadi",
  "repository": {
    "type": "git",
    "url": "https://github.com/m-ahmadi/pixi.git"
  },
  "license": "MIT",
  "devDependencies": {
    "@types/jquery": "^2.0.39",
    "@types/pixi.js": "^4.3.1",
    "babel-cli": "^6.23.0",
    "babel-preset-env": "^1.2.0",
    "babel-preset-es2015": "^6.22.0",
    "body-parser": "^1.17.1",
    "browserify": "^13.3.0",
    "chokidar": "^1.6.1",
    "colors": "^1.1.2",
    "concat": "^3.0.0",
    "copy-dir": "^0.3.0",
    "express": "^4.14.0",
    "find-in-files": "^0.4.0",
    "glob": "^7.1.1",
    "grunt": "^1.0.1",
    "grunt-browserify": "^5.0.0",
    "grunt-contrib-concat": "^1.0.1",
    "grunt-contrib-jshint": "^1.0.0",
    "grunt-contrib-sass": "^1.0.0",
    "grunt-contrib-watch": "^1.0.0",
    "grunt-jslint": "^1.1.14",
    "grunt-shell": "^2.1.0",
    "grunt-webpack": "^2.0.0-beta.6",
    "gulp": "^3.9.1",
    "gulp-changed": "^2.0.0",
    "gulp-livereload": "^3.8.1",
    "gulp-shell": "^0.6.3",
    "handlebars": "^4.0.6",
    "indent.js": "^0.1.4",
    "jshint-stylish": "^2.2.1",
    "line-by-line": "^0.1.5",
    "requirejs": "^2.3.3",
    "shelljs": "^0.7.7",
    "socket.io": "^1.7.2",
    "tsify": "^3.0.0",
    "typescript": "^2.1.5",
    "util-ma": "^1.0.9",
    "watchify": "^3.8.0",
    "webpack": "^2.2.0-rc.3"
  },
  "dependencies": {}
}
